<%- include('header'); -%>
    <div class="box" id="heading">
            <h1><%= listTitle %></h1>
    </div>
    <div class="box">
            <% for (var key in newListItems) { %>
        <div class="item">
            <input type="checkbox">
                <p><%= newListItems[key] %></p>
                
        </div>
        <% } %>
        <form class="item" action="/" method="POST">
            <input type="text" placeholder="New Item" name="newItem" autocomplete="off">
            <button type="submit" name="list" value=<%= listTitle %>>+</button>
            </form>
    </div>
    <%- include('footer'); -%>